  {% for post in site.posts %}
  <item>
    <title>{{ post.title }}</title>
    <description><![CDATA[{{ post.content }}]]></description>
    <guid>{{ post.url }}</guid>
    <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:00 -0400" }}</pubDate>
  </item>
  {% endfor %}
</channel>
</rss>
