<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
  <title>code.nevercraft.net</title>
  <description>My blog</description>
  <link>http://code.nevercraft.net</link>
  <!-- <lastBuildDate>Mon, 06 Sep 201 00:01:00 +0000 </lastBuildDate>
